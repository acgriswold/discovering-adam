---
import { storyblokEditable } from "@storyblok/astro";

import { NavigationWrapper } from "@/components/global/navigationWrapper";

const { blok } = Astro.props;
---


<NavigationWrapper 
    trigger={"Introduction"}
    title={blok.HomeTitle}
    description={blok.HomeDescription}
    href={blok.HomeHref}
    primaryLinks={blok.PrimaryLinks}
    secondaryItems={blok.SecondaryItems}

    {...storyblokEditable(blok)} 
    client:load />